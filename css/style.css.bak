/*-------------------------------slide show  ----------------------------------------*/
.slide {
  width: 100%;
  height: 350px;
  overflow: hidden;
  position: relative;
  background-color: #000;
}
@media screen and (max-width: 600px) {
	.slide {
  width: 100%;
  height: 150px;
	}
}

.slide > div {
  width: 100%;
  height: 100%;
  background-size: 100% 350px;
  position: absolute;
  animation: slide 25s infinite;
  opacity: 0;
}
@media screen and (max-width: 600px) {
	.slide > div {
  width: 100%;
  height: 100%;
   background-size: 100% 150px;
	}
}


.slide > div:nth-child(2) {
  animation-delay: 5s;
}
.slide > div:nth-child(3) {
  animation-delay: 10s;
}
.slide > div:nth-child(4) {
  animation-delay: 15s;
}
.slide > div:nth-child(5) {
  animation-delay: 20s;
}
@keyframes slide {
  10% {
    opacity: 1;
  }
  20% {
    opacity: 1;
  }
  30% {
    opacity: 0;
  }
  40% {
    transform: scale(1.15);
  }
}



