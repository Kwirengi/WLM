/* Style the tab */
.tab {
  overflow: hidden;
}

/* Style the buttons that are used to open the tab content */
.tab button {
	color: green;
  font-size: 18px;
  background-color: inherit;
  float: left;
  border: none;
  outline: none;
  cursor: pointer;
  padding: 14px 16px;
  transition: 0.3s;
}
.tab button:hover {
	background-color: rgba(255,255,0);
}
.tabcontent{transition: 0.3s;}



/* Create an active/current tablink class */
.tab button.active {
  background-color: rgba(0,200,0);
}
@media screen and (max-width: 600px) {
	.tab button {
		padding: 7px 8px;
		font-size: 11px;
	}
}
